{
  "name": "Java! Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "자바! 도우미 확장",
  "permissions": ["tabs", "activeTab", "storage", "declarativeContent"],
  "page_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
